{
  "name": "boilerplate-reactionic-meteor",
  "private": true,
  "main": "server/main.js",
  "browser": "client/main.js",
  "scripts": {
    "start": "meteor run",
    "lint": "eslint ."
  },
  "dependencies": {
    "autoprefixer": "^6.3.1",
    "classnames": "^2.2.3",
    "faker": "^3.0.1",
    "jquery": "^2.2.3",
    "lodash": "^4.5.1",
    "meteor-node-stubs": "^0.2.3",
    "mobx": "^2.5.1",
    "mobx-react": "^3.5.5",
    "moment": "^2.13.0",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-addons-pure-render-mixin": "^15.0.2",
    "react-addons-update": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "react-static-container": "^1.0.0",
    "reactionic": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-import-resolver-meteor": "^0.2.3",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-meteor": "^3.4.0",
    "eslint-plugin-react": "^4.3.0",
    "gm": "^1.21.1",
    "webpack": "^1.13.0",
    "webpack-hot-middleware": "^2.10.0",
    "url-loader": "^0.5.7",
    "file-loader": "^0.8.5",
    "style-loader": "^0.13.0",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "json-loader": "^0.5.4",
    "sass-loader": "^3.1.2",
    "node-sass": "^3.4.2",
    "babel": "^6.3.26",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "expose-loader": "^0.7.1",
    "react-transform-hmr": "^1.0.1",
    "react-transform-catch-errors": "^1.0.0",
    "redbox-react": "^1.2.0",
    "postcss-loader": "^0.8.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "meteor",
      "react"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended",
      "plugin:react/recommended"
    ],
    "globals": {
      "server": false,
      "browser": false,
      "expect": false
    },
    "rules": {
      "react/display-name": 0,
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true,
          "allow": [
            "_id",
            "_ensureIndex",
            "_verifyEmailToken",
            "_resetPasswordToken",
            "_name"
          ]
        }
      ]
    },
    "settings": {
      "import/resolver": "meteor"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  }
}
